{
  "name": "@edmit/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "graphql": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn introspect-schema && yarn generate-typings",
    "introspect-schema": "node -r dotenv/config scripts/introspectSchema.js dotenv_config_path=.env.development",
    "generate-typings": "apollo codegen:generate src/graphql/generated.ts --queries=src/graphql/*.ts --schema=src/graphql/schema.json --addTypename --target=typescript --outputFlat",
    "test": "react-app-rewired test --env=jsdom --modulePaths=."
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
